<Project>
  <PropertyGroup>
    <!--<TargetFrameworks>net6.0;netstandard2.1;netstandard2.0</TargetFrameworks>-->
    <TargetFramework>net6.0</TargetFramework>
    <LangVersion>11</LangVersion>
    <Nullable>enable</Nullable>
    <Authors>linkoid</Authors>
    <VersionPrefix>0.1.0</VersionPrefix>
    <IsPackable>false</IsPackable>
  </PropertyGroup>
  <PropertyGroup>
    <PackageIdPrefix>Linkoid.</PackageIdPrefix>
    <Version>$(VersionPrefix)-alpha</Version>
  </PropertyGroup>

  <PropertyGroup>
    <DefineConstants Condition="
                     '$(TargetFramework)' == 'net6.0'
                     or '$(TargetFramework)' == 'netstandard2.1'"
      >$(DefineConstants);IMPLEMENTS_NETSTANDARD2_1_OR_GREATER</DefineConstants>
  </PropertyGroup>
</Project>
